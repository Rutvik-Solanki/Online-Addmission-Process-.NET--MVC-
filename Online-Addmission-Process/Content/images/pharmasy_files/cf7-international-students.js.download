jQuery('[name=iep_student_of]').change(function() {
	console.log(jQuery(this).val());
	jQuery('[name=iep_course]').find('option').remove();
	jQuery('[name=iep_course]').append('<option value=""></option>');
	if (jQuery(this).val() == 'Marwadi University') {
		for (var i = 0; i < course_list_mu.length; i++) {
			jQuery('[name=iep_course]').append('<option value="'+course_list_mu[i]+'">'+course_list_mu[i]+'</option>');
		}
	} else if (jQuery(this).val() == 'MEFGI under GTU') {
		for (var i = 0; i < course_list_mefgi.length; i++) {
			jQuery('[name=iep_course]').append('<option value="'+course_list_mefgi[i]+'">'+course_list_mefgi[i]+'</option>');
		}
	}
});

jQuery('[name=iep_course]').change(function() {
	console.log(jQuery(this).val());
	jQuery('[name=iep_yos]').find('option').remove();
	jQuery('[name=iep_yos]').append('<option value=""></option>');
	jQuery('[name=iep_sos]').find('option').remove();
	jQuery('[name=iep_sos]').append('<option value=""></option>');
	for (var i = 1; i <= 10; i++) {
		jQuery('p#p_iep_grade_'+i).hide(0);
	}
	for (var i = 0; i < yos_list.length; i++) {
		jQuery('[name=iep_yos]').append('<option value="'+yos_list[i]+'">'+yos_list[i]+'</option>');
	}
});

jQuery('[name=iep_yos]').change(function() {
	console.log(jQuery(this).val());
	jQuery('[name=iep_sos]').find('option').remove();
	jQuery('[name=iep_sos]').append('<option value=""></option>');
	for (var i = 1; i <= 10; i++) {
		jQuery('p#p_iep_grade_'+i).hide(0);
	}
	var yos = parseInt(jQuery(this).val());
	for (var i = (yos-1)*2+1; i <= yos*2; i++) {
		jQuery('[name=iep_sos]').append('<option value="'+i+'">'+i+'</option>');
	}
});

jQuery('[name=iep_sos]').change(function() {
	console.log(jQuery(this).val());
	jQuery('[name=iep_university]').find('option').remove();
	jQuery('[name=iep_university]').append('<option value=""></option>');
	for (var i = 1; i <= 10; i++) {
		jQuery('p#p_iep_grade_'+i).hide(0);
	}
	var sos = parseInt(jQuery(this).val());
	for (var i = 1; i <= sos; i++) {
		jQuery('p#p_iep_grade_'+i).show(0);
	}
	for (var i = 0; i < university_list.length; i++) {
		jQuery('[name=iep_university]').append('<option value="'+university_list[i]+'">'+university_list[i]+'</option>');
	}
});

jQuery('[name=iep_university]').change(function() {
	console.log(jQuery(this).val());
	jQuery('[name=iep_programme]').find('option').remove();
	jQuery('[name=iep_programme]').append('<option value=""></option>');
	jQuery('[name=iep_module]').find('option').remove();
	jQuery('[name=iep_module]').append('<option value=""></option>');
	jQuery('p#p_iep_module').hide(0);
	jQuery('p#p_iep_date').hide(0);
