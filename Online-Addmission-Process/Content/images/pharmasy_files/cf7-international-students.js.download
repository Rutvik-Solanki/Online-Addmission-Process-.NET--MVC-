		jQuery('[name=iep_module]').append('<option value="'+this_module_list[i]+'">'+this_module_list[i]+'</option>');
	}
});

jQuery('[name=iep_module]').change(function() {
	console.log(jQuery(this).val());
	var this_module_details = module_details_list[jQuery('[name=iep_university]').val()][jQuery('[name=iep_programme]').val()][jQuery(this).val()];
	console.log(this_module_details);
	jQuery('span#span_iep_module').html(jQuery(this).val());
	jQuery('p#p_iep_module').show(0);
	jQuery('[name=iep_date]').val(this_module_details.date)
	jQuery('span#span_iep_date').html(this_module_details.date);
	jQuery('p#p_iep_date').show(0);
	jQuery('[name=iep_fees]').val(this_module_details.fee)
	jQuery('span#span_iep_fees').html(this_module_details.fee);
	jQuery('p#p_iep_fees').show(0);
	jQuery('[name=iep_inclusions]').val(this_module_details.inclusions)
	jQuery('span#span_iep_inclusions').html(this_module_details.inclusions);
	jQuery('p#p_iep_inclusions').show(0);
	jQuery('[name=iep_exclusions]').val(this_module_details.exclusions)
	jQuery('span#span_iep_exclusions').html(this_module_details.exclusions);
