		jQuery('[name=iep_programme]').append('<option value="'+this_programme_list[i]+'">'+this_programme_list[i]+'</option>');
	}
});

jQuery('[name=iep_programme]').change(function() {
	console.log(jQuery(this).val());
	jQuery('[name=iep_module]').find('option').remove();
	jQuery('[name=iep_module]').append('<option value=""></option>');
	jQuery('p#p_iep_module').hide(0);
	jQuery('p#p_iep_date').hide(0);
